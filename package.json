{
    "name": "muse-typescript",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "migration": "nodemon --watch ./src --exec ts-node src/migrations/index.ts NODE_ENV=dev",
        "start": "NODE_ENV=dev nodemon --watch ./src --exec ts-node src/index.ts",
        "build": "tsc",
        "prod": "npm run build && node build/src/index.js",
        "react_link": "rm -rf src/public/site/react_assets/* && rm -rf src/public/admin/admin-ui/* && node react-ui/react_includer/index.js",
        "generate": "node builder/index.js",
        "db:migrate": "ts-node ./migrations.js",
        "pg:db:drop": "dropdb eco_dev",
        "pg:db:create": "createdb -O eco eco_dev",
        "test": "jest --detectOpenHandles --maxWorks=10 --forceExit"
    },
    "dependencies": {
        "@types/express-handlebars": "^3.1.0",
        "@types/nodemailer": "^6.4.0",
        "add": "^2.0.6",
        "axios": "^0.19.0",
        "bcrypt": "^3.0.7",
        "bluebird": "^3.5.5",
        "body-parser": "^1.19.0",
        "cpf-check": "^3.0.0",
        "date-and-time": "^0.10.0",
        "express": "^4.17.1",
        "express-handlebars": "^3.1.0",
        "express-http-to-https": "^1.1.4",
        "express-session": "^1.16.2",
        "gox-packages": "^1.0.4",
        "handlebars": "4.5.3",
        "jsonwebtoken": "^8.5.1",
        "knex": "~0.20.4",
        "module-alias": "^2.2.1",
        "moment-timezone": "^0.5.26",
        "mysql": "^2.17.1",
        "node-fs-extra": "^0.8.2",
        "node-nlp": "^3.10.2",
        "nodemailer": "^6.4.2",
        "nodemon": "^1.19.2",
        "passport": "0.4.1",
        "passport-local": "^1.0.0",
        "react-dom": "16.12.0",
        "require.all": "^2.0.4",
        "striptags": "^3.1.1",
        "validar-cpf": "^2.1.1",
        "validate.js": "^0.13.1",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/bcrypt": "^3.0.0",
        "@types/date-and-time": "^0.6.0",
        "@types/express": "^4.17.1",
        "@types/express-session": "^1.15.14",
        "@types/jest": "24.0.23",
        "@types/jsonwebtoken": "^8.3.4",
        "@types/passport": "^1.0.2",
        "@types/passport-local": "^1.0.33",
        "@types/react-dom": "16.9.4",
        "@types/supertest": "2.0.8",
        "dotenv": "^8.1.0",
        "jest": "24.9.0",
        "pg": "7.14.0",
        "supertest": "4.0.2",
        "ts-jest": "24.2.0",
        "ts-node": "^8.3.0",
        "typescript": "~3.7.0"
    },
    "_moduleAliases": {
        "@models": "src/models/",
        "@database": "src/server/db",
        "@providers": "src/providers",
        "@services": "src/services",
        "@logic": "src/logic"
    }
}
